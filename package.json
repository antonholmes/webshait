{
  "name": "shailagarde.com",
  "version": "1.0.0",
  "private": true,
  "description": "Shai Lagarde's newest portal",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "node server",
