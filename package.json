    "babel-preset-env": "^1.7.0",
    "chai": "^3.5.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-fullstack": "^5.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
